{
  "name": "fullpage-swiper",
  "version": "0.1.0",
  "description": "Fullpage-swiper is fullpage movement library for browser via touch actions",
  "main": "dist/fullpage-swiper.min.js",
  "scripts": {
    "build:browser": "webpack && cp dist/fullpage-swiper.min.js examples/browser",
    "example:view": "npm run build:browser && serve examples/browser -p 3000",
    "release": "np"
  },
  "publishConfig": {
    "access": "public"
  },
  "np": {
    "yarn": false,
    "contents": "dist"
  },
  "keywords": [
    "fullpage-swiper",
    "draggable",
    "swipe",
    "gsap",
    "hammerjs"
  ],
  "author": "Catalsrun",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^3.9.1",
    "eslint-loader": "^1.6.1",
    "gsap": "^3.5.1",
    "hammerjs": "^2.0.8",
    "np": "^6.5.0",
    "serve": "^11.3.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "peerDependencies": {
    "gsap": "3.x",
    "hammerjs": "2.x"
  }
}